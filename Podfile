source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '10.0'
use_frameworks!

target :'ExCars' do
    ## Require one of the AeroGear SDK's here
    pod 'SkeletonView'
    pod 'GoogleSignIn'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'KeychainAccess'
    pod 'Starscream'
    pod 'SDWebImage'
    pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git', :branch => 'alamofire5'
    pod 'SideMenu'
end
