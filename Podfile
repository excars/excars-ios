source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'
use_frameworks!

target :'ExCars' do
    ## Require one of the AeroGear SDK's here
    pod 'GoogleSignIn'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'Alamofire', '~> 4.7'
    pod 'KeychainAccess'
    pod 'Starscream'
    pod 'SDWebImage'
end
