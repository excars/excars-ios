source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '10.0'
use_frameworks!
inhibit_all_warnings!

target :'ExCars' do
    ## Require one of the AeroGear SDK's here
    pod 'SkeletonView',         '~> 1.4'
    pod 'GoogleSignIn',         '~> 4.4'
    pod 'GoogleMaps',           '~> 3.0'
    pod 'GooglePlaces',         '~> 3.0'
    pod 'KeychainAccess',       '~> 3.2'
    pod 'Starscream',           '~> 3.1'
    pod 'SDWebImage',           '~> 5.0'
    pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git', :branch => 'alamofire5'
    pod 'SideMenu',             '~> 5.0'
end
